<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="io.choerodon.devops.infra.mapper.DevopsDeployGroupMapper">

    <select id="queryById" resultType="io.choerodon.devops.infra.dto.DevopsDeployGroupDTO">
        SELECT
            *
        FROM
            devops_deploy_group
        WHERE
            project_id = #{projectId}
            AND id = #{devopsConfigGroupId}
    </select>
</mapper>