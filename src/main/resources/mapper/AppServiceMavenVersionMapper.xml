<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="io.choerodon.devops.infra.mapper.AppServiceMavenVersionMapper">

    <select id="listByAppVersionIds"
            resultType="io.choerodon.devops.api.vo.appversion.AppServiceMavenVersionVO">
        select *
        from devops_app_service_maven_version dasmv
        where dasmv.app_service_version_id in
        <foreach collection="versionIds" index="i" open="(" separator="," close=")" item="versionId">
            #{versionId}
        </foreach>
    </select>
</mapper>

