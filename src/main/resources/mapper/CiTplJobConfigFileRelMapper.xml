<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="io.choerodon.devops.infra.mapper.CiTplJobConfigFileRelMapper">

    <select id="listByJobId" resultType="io.choerodon.devops.infra.dto.CiTplJobConfigFileRelDTO">
        select *
        from devops_ci_tpl_job_config_file_rel dctjcfr
        where ci_template_job_id = #{jobId}
        order by id
    </select>
</mapper>

